
private class Rectangle extends GeometricFigure {
    private int height;
    private int width;

    private Rectangle(int height, int width) {
        this.height = height;
        this.width = width;
    }

    @Override
    double getArea() {
        return height * width;
    }

    @Override
    double getPerimeter() {
        return (height + width) * 2;
    }
    private double getDiagonal(){
        return Math.sqrt(Math.pow(width, 2) + Math.pow(height, 2));
    }
    private int getHeight() {
        return height;
    }

    private int getWidth() {
        return width;
    }

    private void setHeight(int height) {
        this.height = height;
    }

    private void setWidth(int width) {
        this.width = width;
    }
}